    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@primeng/themes": "^19.1.3",
    "@splidejs/splide": "^4.1.4",
    "@supabase/supabase-js": "^2.49.8",
    "@swimlane/ngx-charts": "^23.0.0-alpha.0",
    "angular-oauth2-oidc": "^19.0.0",
    "chart.js": "^4.5.0",
    "flyonui": "^2.2.0",
